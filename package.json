{
  "name": "ableron",
  "version": "0.0.1",
  "description": "JavaScript Library for Ableron Server Side UI Composition",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-esm.json && tsc -p tsconfig-types.json",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ssi",
    "uicomposition"
  ],
  "author": "Torsten Blasche <dev@t-blasche.de>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ableron/ableron-js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ableron/ableron-js.git"
  },
  "bugs": {
    "url": "https://github.com/ableron/ableron-js/issues"
  },
  "dependencies": {
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2"
  }
}
