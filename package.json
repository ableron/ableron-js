{
  "name": "@ableron/ableron",
  "version": "1.4.0-alpha.1",
  "description": "JavaScript Library for Ableron Server Side UI Composition",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=19"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.json && move-file dist/esm/index.js dist/esm/index.mjs",
    "build:cjs": "tsc -p tsconfig/tsconfig-cjs.json",
    "build:types": "tsc -p tsconfig/tsconfig-types.json",
    "prettier": "prettier . --write",
    "test": "vitest"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ableron",
    "ssi",
    "server-side-includes",
    "ui-composition",
    "dynamic-ui-composition"
  ],
  "author": "Torsten Blasche <dev@t-blasche.de>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ableron/ableron-js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ableron/ableron-js.git"
  },
  "bugs": {
    "url": "https://github.com/ableron/ableron-js/issues"
  },
  "dependencies": {
    "@isaacs/ttlcache": "1.4.1"
  },
  "devDependencies": {
    "@types/node": "20.10.8",
    "fastify": "4.25.2",
    "move-file-cli": "3.0.0",
    "prettier": "3.1.1",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3",
    "vitest": "1.1.3"
  },
  "prettier": {
    "printWidth": 120,
    "quoteProps": "consistent",
    "singleQuote": true,
    "trailingComma": "none"
  }
}
